<div>


<% if current_user.clinician %>
  <p>I'm a clinician</p>
  <p><%= @appointment.appointment_date %></p>
<% else %>
  <p>I'm a patient</p>
  <p><%= @appointment.appointment_date %></p>
<% end %>

<% if @appointment.chatroom %>
  <%= link_to 'Chat', appointment_chatroom_path(@appointment, @appointment.chatroom) %>
<% end %>

<%= link_to 'Back', appointments_path %>

</div>
